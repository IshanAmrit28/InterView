{
  "compilerOptions": {
    "target": "ES2022",
    "moduleResolution": "node",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "noImplicitAny": false,
    "skipLibCheck": true
  },
  "include": [
    "controllers/**/*",
    "models/**/*",
    "routes/**/*",
    "app.js"
  ]
}